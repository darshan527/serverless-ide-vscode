{
    "name": "@serverless-ide/config",
    "description": "Serverless IDE config model and parser",
    "version": "0.6.0",
    "author": "Pavel Vlasov <freakycue@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "main": "dist/index.js",
    "keywords": [
        "aws",
        "sam",
        "cloudformation",
        "cfn",
        "serverless",
        "yaml",
        "autocompletion",
        "validation",
        "LSP"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/threadheap/serverless-ide-vscode.git"
    },
    "scripts": {
