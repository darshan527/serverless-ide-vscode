        "vscode-languageserver": "^5.2.1",
        "vscode-languageserver-types": "^3.14.0",
        "vscode-nls": "^3.2.2",
        "yaml-ast-parser-custom-tags": "0.0.43"
    },
    "devDependencies": {
        "@types/js-yaml": "^3.12.1",
        "@types/lodash": "^4.14.144",
        "@types/lru-cache": "^4.1.1",
        "@types/node": "^9.4.7",
        "@types/vscode": "^1.37.0",
        "typescript": "^4.9.4"
